{
  "name": "www",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "clean": "rm -rf ./dist && mkdir ./dist",
    "clean-package": "rm -rf ./lambda && mkdir ./lambda && rm -rf ./lambda-layer && mkdir ./lambda-layer",
    "package-prep-copy-function": "cp -r ./dist/* ./lambda",
    "package-prep-copy-layer": "cp ./package.json ./lambda-layer && cp ./package-lock.json ./lambda-layer",
    "package-prep-copy": "npm run clean-package && npm package-prep-copy-function && npm run package-prep-copy-layer",
    "package-prep-layer-install": "cd ./lambda-layer && npm ci --omit=dev && mkdir nodejs && mv node_modules ./nodejs/",
    "package-prep": "npm run package-prep-copy && npm run package-prep-layer-install",
    "package-function": "cd ./lambda && zip -r ../lambda_function.zip ./",
    "package-layer": "cd ./lambda-layer && rm *.json && zip -r ../lambda-layer.zip ./",
    "package": "npm run package-prep && npm run package-function && npm run package-layer",
    "astro": "astro"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
     "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@tailwindcss/vite": "^4.0.3",
    "astro": "^5.2.3",
    "astro-sst": "^2.45.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "daisyui": "^5.0.0-beta.6",
    "tailwindcss": "^4.0.3"
  }
}
